    "globals": "^15.10.0",
    "husky": "^9.1.6",
    "jest": "^27.5.1",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.2.10",
    "postcss": "^8.4.48",
    "prettier": "^3.3.3",
    "tailwindcss": "^3.4.14",
    "typescript-eslint": "^8.8.1"
  },
  "lint-staged": {
    ".{js,jsx,ts,tsx}": [
      "eslint --cache --fix",
      "prettier --write"
    ],
    "*.{json,css,md}": [
      "prettier --write"
    ]
  }
