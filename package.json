    "eject": "react-scripts eject",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "prepare": "husky",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\"",
